{
  "name": "tm/php-metrics-monitor",
  "description": "Simple tool to visualize metrics.",
  "keywords": [
    "metrics",
    "xml",
    "monitor",
    "coverage"
  ],
  "support": {
    "email": "tommy.muehle@gmail.com",
    "issues": "https://github.com/tommy-muehle/php-metrics-monitor/issues",
    "docs": "https://github.com/tommy-muehle/php-metrics-monitor/blob/master/README.md",
    "source": "https://github.com/tommy-muehle/php-metrics-monitor/tree/master"
  },
  "authors": [
    {
      "name": "Tommy Muehle",
      "email": "tommy.muehle@gmail.com",
      "homepage": "https://tommy-muehle.de"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "ext-dom": "*",
    "silex/silex": "^1.0",
    "symfony/console": "^3.0",
    "symfony/process": "^3.0",
    "twig/twig": "1.9.*",
    "doctrine/dbal": "2.5.4",
    "erusev/parsedown": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3.0",
    "symfony/browser-kit": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "MetricsMonitor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MetricsMonitor\\": "src/",
      "MetricsMonitor\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
